@model List<AccompanyingDrugPreviewViewModel>
@inject MedicDataLocalization medicDataLocalization

@if (Model != default && Model.Count > 0)
{
    @foreach (var currentAccProtocol in Model)
    {
        <hr />
        <div>
            <div class="row">
                <div class="col-sm-3">@Html.Raw(medicDataLocalization.Get("ATCCode")): @currentAccProtocol.ATCCode</div>
                <div class="col-sm-3">@Html.Raw(medicDataLocalization.Get("ATCName")): @currentAccProtocol.ATCName</div>
                <div class="col-sm-3">@Html.Raw(medicDataLocalization.Get("SingleDose")): @currentAccProtocol.SingleDose</div>
                <div class="col-sm-3">@Html.Raw(medicDataLocalization.Get("AllDose")): @currentAccProtocol.AllDose</div>
            </div>
            <partial name="~/Views/Shared/Partials/TherapyTypes/TherapyTypePreviewViewModel.cshtml" model="@currentAccProtocol.TherapyType" />
        </div>
    }
}
else
{
    <div class="text-center text-info">@Html.Raw(medicDataLocalization.Get("NoResults"))</div>
}