@model TherapyTypePreviewViewModel
@inject MedicDataLocalization medicDataLocalization

@if (Model != default)
{
    <div>
        <div class="row">
            <div class="col-sm-6">@medicDataLocalization.Get("Code"): @Model.Code</div>
            <div class="col-sm-6">@medicDataLocalization.Get("Name"): @Model.Name</div>
        </div>
    </div>
}
else
{
    <div class="text-center text-info">@medicDataLocalization.Get("NoResults")</div>
}